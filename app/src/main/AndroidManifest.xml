<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.excellence.skinloader"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:name=".SkinLoaderApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.SkinBySuffixActivity">
        </activity>
        <activity android:name=".activity.SkinByAPKFileActivity">
        </activity>
        <activity android:name=".activity.SkinByInstalledAPKActivity">
        </activity>
        <activity android:name=".activity.SkinLanguageActivity">
        </activity>
        <activity android:name=".activity.SkinTextSizeActivity">
        </activity>
    </application>

</manifest>